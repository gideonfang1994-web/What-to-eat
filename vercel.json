{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    {
      "source": "/api/menu/:chefId",
      "destination": "/api/menu?chefId=:chefId"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
